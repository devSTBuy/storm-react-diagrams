{
	"name": "storm-react-diagrams",
	"version": "5.2.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/leowilsoncordones/react-diagrams.git"
	},
	"keywords": [
		"web",
		"diagram",
		"diagrams",
		"react",
		"typescript",
		"flowchart",
		"simple",
		"links",
		"nodes"
	],
	"main": "./dist/main.js",
	"typings": "./dist/@types/src/main",
	"author": "dylanvorster",
	"scripts": {
		"build": "npm run build:ts:prod && npm run build:sass:prod",
		"build:ts": "webpack",
		"build:ts:prod": "cross-env NODE_ENV=production webpack",
		"build:sass:prod": "sass --no-source-map ./src/sass/main.scss ./dist/style.min.css --style compressed",
		"storybook": "start-storybook -p 9001 -c .storybook",
		"storybook:build": "build-storybook -c .storybook -o .out",
		"storybook:github": "storybook-to-ghpages",
		"pretty": "prettier --use-tabs --write \"{src,demos,tests}/**/*.{ts,tsx}\" --print-width 120",
		"lint": "tslint -p .",
		"test:ci": "rm -rf ./dist && node ./tests/e2e/generate-e2e.js && jest --no-cache",
		"test": "jest --no-cache",
		"prepublishOnly": "yarn run build:ts:prod && yarn run build:sass:prod"
	},
	"dependencies": {
		"closest": "^0.0.1",
		"copy-webpack-plugin": "^11.0.0",
		"lodash": "^4.17.10",
		"pathfinding": "^0.4.18",
		"paths-js": "^0.4.7",
		"react": "^16.3.2",
		"react-scripts": "5.0.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^3.4.3",
		"@storybook/addon-options": "^3.4.3",
		"@storybook/addon-storyshots": "^3.4.3",
		"@storybook/addons": "^3.4.3",
		"@storybook/react": "^3.4.3",
		"@storybook/storybook-deployer": "^2.3.0",
		"@types/jest": "^22.2.3",
		"@types/lodash": "^4.14.108",
		"@types/node": "^10.0.4",
		"@types/promise": "^7.1.30",
		"@types/puppeteer": "^1.3.1",
		"@types/react": "^16.3.13",
		"cross-env": "^5.1.4",
		"css-loader": "^6.8.1",
		"dagre": "^0.8.2",
		"enzyme": "^3.3.0",
		"file-loader": "^6.2.0",
		"glob": "^7.1.2",
		"html-webpack-plugin": "^5.5.3",
		"jest": "^22.4.3",
		"jest-cli": "^22.4.3",
		"json-beautify": "^1.0.1",
		"prettier": "^1.12.1",
		"puppeteer": "^1.3.0",
		"raf": "^3.4.0",
		"raw-loader": "^0.5.1",
		"react-dom": "^16.3.2",
		"react-syntax-highlighter": "^7.0.4",
		"react-test-renderer": "^16.3.2",
		"sass": "^1.62.0",
		"sass-loader": "9.0.3",
		"source-map-loader": "^0.2.3",
		"storybook-host": "^4.1.5",
		"storybook-readme": "^3.3.0",
		"style-loader": "^0.21.0",
		"terser-webpack-plugin": "^5.3.9",
		"ts-jest": "^23.10.0",
		"ts-loader": "8.2.0",
		"tslint": "^6.1.3",
		"typescript": "^4.7.3",
		"val-loader": "^5.0.1"
	}
}
