{
	"compileOnSave": false,
	"name": "storm-react-diagrams",
	"compilerOptions": {
		"declaration": true,
		"outDir": "@types",
		"target": "es5",
		"strictNullChecks": false,
		"sourceMap": true,
		"skipLibCheck": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"storm-react-diagrams": ["src/main.ts"]
		},
		"lib": ["dom", "es2015", "es2017.object"]
	},
	"exclude": ["node_modules", "**/*.spec.ts", "./dist"]
}
